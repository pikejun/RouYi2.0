    <delete id="deleteBiScopeComplaintLogByIds" parameterType="String">
        delete from bi_scope_complaint_log where tid in 
        <foreach item="tid" collection="array" open="(" separator="," close=")">
            #{tid}
        </foreach>
    </delete>


    <update id="updateBiScopeComplaintLogReadyToDeal" parameterType="Map">
        update bi_scope_complaint_log set op_status=3,updated_by=#{host},updated_time=now() where op_status=1 limit 100
    </update>



    <update id="updateBiScopeComplaintLogToFailed" parameterType="Map">
        update bi_scope_complaint_log set op_status=9,updated_by=#{host},updated_time=now() where tid=#{tid}
    </update>

    <update id="updateBiScopeComplaintLogToComplate" parameterType="Map">
        update bi_scope_complaint_log set op_status=2,updated_by=#{host},updated_time=now() where tid=#{tid}
    </update>


    <select id="selectBiScopeComplaintLogReadToDeal" parameterType="Map" resultType="com.ruoyi.project.system.biScopeComplaintLog.vo.BiScopeComplaintLogVO">
        select
        tid, id, complaint_id complaintId, content, create_date createDate, sender, replay_id replayId, replay_name replayName,
        op_status opStatus, op_type opType, created_time createdTime, created_by createdBy, updated_time updatedTime, updated_by updatedBy

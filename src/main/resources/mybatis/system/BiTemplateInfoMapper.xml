<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.project.system.biTemplateInfo.mapper.BiTemplateInfoMapper">
    
    <resultMap type="BiTemplateInfo" id="BiTemplateInfoResult">
        <result property="templateId"    column="template_id"    />
        <result property="templateNo"    column="template_no"    />
        <result property="senderIndexNo"    column="sender_index_no"    />
        <result property="templateName"    column="template_name"    />
        <result property="templateContent"    column="template_content"    />
        <result property="status"    column="status"    />
        <result property="createdTime"    column="created_time"    />
        <result property="createdBy"    column="created_by"    />
        <result property="updatedTime"    column="updated_time"    />
        <result property="updatedBy"    column="updated_by"    />
    </resultMap>
	
	<sql id="selectBiTemplateInfoVo">
        select template_id, template_no, sender_index_no, template_name, template_content, status, created_time, created_by, updated_time, updated_by from bi_template_info
    </sql>
	
    <select id="selectBiTemplateInfoList" parameterType="BiTemplateInfo" resultMap="BiTemplateInfoResult">
        <include refid="selectBiTemplateInfoVo"/>
        <where>  
            <if test="templateId != null "> and template_id = #{templateId}</if>
             <if test="templateNo != null  and templateNo != '' "> and template_no = #{templateNo}</if>
             <if test="senderIndexNo != null  and senderIndexNo != '' "> and sender_index_no = #{senderIndexNo}</if>
             <if test="templateName != null  and templateName != '' "> and template_name = #{templateName}</if>
             <if test="templateContent != null  and templateContent != '' "> and template_content = #{templateContent}</if>
             <if test="status != null "> and status = #{status}</if>
             <if test="createdTime != null "> and created_time = #{createdTime}</if>
             <if test="createdBy != null  and createdBy != '' "> and created_by = #{createdBy}</if>
             <if test="updatedTime != null "> and updated_time = #{updatedTime}</if>
             <if test="updatedBy != null  and updatedBy != '' "> and updated_by = #{updatedBy}</if>
         </where>
    </select>
    
    <select id="selectBiTemplateInfoById" parameterType="Integer" resultMap="BiTemplateInfoResult">
        <include refid="selectBiTemplateInfoVo"/>
        where template_id = #{templateId}
    </select>
        
    <insert id="insertBiTemplateInfo" parameterType="BiTemplateInfo" useGeneratedKeys="true" keyProperty="templateId">
        insert into bi_template_info
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="templateNo != null  and templateNo != ''  ">template_no,</if>
			<if test="senderIndexNo != null  and senderIndexNo != ''  ">sender_index_no,</if>
			<if test="templateName != null  and templateName != ''  ">template_name,</if>
			<if test="templateContent != null  and templateContent != ''  ">template_content,</if>
			<if test="status != null  ">status,</if>
			<if test="createdTime != null  ">created_time,</if>
			<if test="createdBy != null  and createdBy != ''  ">created_by,</if>
			<if test="updatedTime != null  ">updated_time,</if>
			<if test="updatedBy != null  and updatedBy != ''  ">updated_by,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="templateNo != null  and templateNo != ''  ">#{templateNo},</if>
			<if test="senderIndexNo != null  and senderIndexNo != ''  ">#{senderIndexNo},</if>
			<if test="templateName != null  and templateName != ''  ">#{templateName},</if>
			<if test="templateContent != null  and templateContent != ''  ">#{templateContent},</if>
			<if test="status != null  ">#{status},</if>
			<if test="createdTime != null  ">#{createdTime},</if>
			<if test="createdBy != null  and createdBy != ''  ">#{createdBy},</if>
			<if test="updatedTime != null  ">#{updatedTime},</if>
			<if test="updatedBy != null  and updatedBy != ''  ">#{updatedBy},</if>
         </trim>
    </insert>
	 
    <update id="updateBiTemplateInfo" parameterType="BiTemplateInfo">
        update bi_template_info
        <trim prefix="SET" suffixOverrides=",">
            <if test="templateNo != null  and templateNo != ''  ">template_no = #{templateNo},</if>
            <if test="senderIndexNo != null  and senderIndexNo != ''  ">sender_index_no = #{senderIndexNo},</if>
            <if test="templateName != null  and templateName != ''  ">template_name = #{templateName},</if>
            <if test="templateContent != null  and templateContent != ''  ">template_content = #{templateContent},</if>
            <if test="status != null  ">status = #{status},</if>
            <if test="createdTime != null  ">created_time = #{createdTime},</if>
            <if test="createdBy != null  and createdBy != ''  ">created_by = #{createdBy},</if>
            <if test="updatedTime != null  ">updated_time = #{updatedTime},</if>
            <if test="updatedBy != null  and updatedBy != ''  ">updated_by = #{updatedBy},</if>
        </trim>
        where template_id = #{templateId}
    </update>

	<delete id="deleteBiTemplateInfoById" parameterType="Integer">
        delete from bi_template_info where template_id = #{templateId}
    </delete>
	
    <delete id="deleteBiTemplateInfoByIds" parameterType="String">
        delete from bi_template_info where template_id in 
        <foreach item="templateId" collection="array" open="(" separator="," close=")">
            #{templateId}
        </foreach>
    </delete>
    
</mapper>
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.project.system.biTemplateIndexProperty.mapper.BiTemplateIndexPropertyMapper">
    
    <resultMap type="BiTemplateIndexProperty" id="BiTemplateIndexPropertyResult">
        <result property="tipId"    column="tip_id"    />
        <result property="templateNo"    column="template_no"    />
        <result property="indexNo"    column="index_no"    />
        <result property="propertyName"    column="property_name"    />
        <result property="defaultValue"    column="default_value"    />
        <result property="orderNum"    column="order_num"    />
        <result property="createdTime"    column="created_time"    />
        <result property="createdBy"    column="created_by"    />
        <result property="updatedTime"    column="updated_time"    />
        <result property="updatedBy"    column="updated_by"    />
    </resultMap>
	
	<sql id="selectBiTemplateIndexPropertyVo">
        select tip_id, template_no, index_no, property_name, default_value, order_num, created_time, created_by, updated_time, updated_by from bi_template_index_property
    </sql>
	
    <select id="selectBiTemplateIndexPropertyList" parameterType="BiTemplateIndexProperty" resultMap="BiTemplateIndexPropertyResult">
        <include refid="selectBiTemplateIndexPropertyVo"/>
        <where>  
            <if test="tipId != null "> and tip_id = #{tipId}</if>
             <if test="templateNo != null  and templateNo != '' "> and template_no = #{templateNo}</if>
             <if test="indexNo != null  and indexNo != '' "> and index_no = #{indexNo}</if>
             <if test="propertyName != null  and propertyName != '' "> and property_name = #{propertyName}</if>
             <if test="defaultValue != null  and defaultValue != '' "> and default_value = #{defaultValue}</if>
             <if test="orderNum != null "> and order_num = #{orderNum}</if>
             <if test="createdTime != null "> and created_time = #{createdTime}</if>
             <if test="createdBy != null  and createdBy != '' "> and created_by = #{createdBy}</if>
             <if test="updatedTime != null "> and updated_time = #{updatedTime}</if>
             <if test="updatedBy != null  and updatedBy != '' "> and updated_by = #{updatedBy}</if>
         </where>
    </select>
    
    <select id="selectBiTemplateIndexPropertyById" parameterType="Integer" resultMap="BiTemplateIndexPropertyResult">
        <include refid="selectBiTemplateIndexPropertyVo"/>
        where tip_id = #{tipId}
    </select>
        
    <insert id="insertBiTemplateIndexProperty" parameterType="BiTemplateIndexProperty">
        insert into bi_template_index_property
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="tipId != null  ">tip_id,</if>
			<if test="templateNo != null  and templateNo != ''  ">template_no,</if>
			<if test="indexNo != null  and indexNo != ''  ">index_no,</if>
			<if test="propertyName != null  and propertyName != ''  ">property_name,</if>
			<if test="defaultValue != null  and defaultValue != ''  ">default_value,</if>
			<if test="orderNum != null  ">order_num,</if>
			<if test="createdTime != null  ">created_time,</if>
			<if test="createdBy != null  and createdBy != ''  ">created_by,</if>
			<if test="updatedTime != null  ">updated_time,</if>
			<if test="updatedBy != null  and updatedBy != ''  ">updated_by,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="tipId != null  ">#{tipId},</if>
			<if test="templateNo != null  and templateNo != ''  ">#{templateNo},</if>
			<if test="indexNo != null  and indexNo != ''  ">#{indexNo},</if>
			<if test="propertyName != null  and propertyName != ''  ">#{propertyName},</if>
			<if test="defaultValue != null  and defaultValue != ''  ">#{defaultValue},</if>
			<if test="orderNum != null  ">#{orderNum},</if>
			<if test="createdTime != null  ">#{createdTime},</if>
			<if test="createdBy != null  and createdBy != ''  ">#{createdBy},</if>
			<if test="updatedTime != null  ">#{updatedTime},</if>
			<if test="updatedBy != null  and updatedBy != ''  ">#{updatedBy},</if>
         </trim>
    </insert>
	 
    <update id="updateBiTemplateIndexProperty" parameterType="BiTemplateIndexProperty">
        update bi_template_index_property
        <trim prefix="SET" suffixOverrides=",">
            <if test="templateNo != null  and templateNo != ''  ">template_no = #{templateNo},</if>
            <if test="indexNo != null  and indexNo != ''  ">index_no = #{indexNo},</if>
            <if test="propertyName != null  and propertyName != ''  ">property_name = #{propertyName},</if>
            <if test="defaultValue != null  and defaultValue != ''  ">default_value = #{defaultValue},</if>
            <if test="orderNum != null  ">order_num = #{orderNum},</if>
            <if test="createdTime != null  ">created_time = #{createdTime},</if>
            <if test="createdBy != null  and createdBy != ''  ">created_by = #{createdBy},</if>
            <if test="updatedTime != null  ">updated_time = #{updatedTime},</if>
            <if test="updatedBy != null  and updatedBy != ''  ">updated_by = #{updatedBy},</if>
        </trim>
        where tip_id = #{tipId}
    </update>

	<delete id="deleteBiTemplateIndexPropertyById" parameterType="Integer">
        delete from bi_template_index_property where tip_id = #{tipId}
    </delete>
	
    <delete id="deleteBiTemplateIndexPropertyByIds" parameterType="String">
        delete from bi_template_index_property where tip_id in 
        <foreach item="tipId" collection="array" open="(" separator="," close=")">
            #{tipId}
        </foreach>
    </delete>
    
</mapper>